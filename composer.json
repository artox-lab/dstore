{
    "name": "artox-lab/dstore",
    "description": "Abstracts for store documents to redis, index them and easy control consistency.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Turchin",
            "email": "a.turchin@artox.com"
        }
    ],
    "require": {
        "predis/predis": "^1.1",
        "ext-json": "*",
        "artox-lab/entities": "dev-master",
        "symfony/validator": "5.2.*",
        "psr/log": "^1.1@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "ArtoxLab\\DStore\\": "src/",
            "ArtoxLab\\DStore\\Examples\\": "examples/"
        },
        "exclude-from-classmap": [
            "/Examples/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ArtoxLab\\DStore\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
